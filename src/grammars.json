{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control",
					"match": "\\b(DEF|USE|ROUTE|TO|PROTO|EXTERNPROTO|PROFILE|IS)\\b"
				},
				{
					"name": "strong",
					"match": "^#X3D V[\\d\\.]+ utf8"
				},
				{
					"name": "constant.character",
					"match": "\\b(TRUE|FALSE|NULL)\\b"
				},
				{
					"name": "comment.line",
					"match": "#.+$"
				},
				{
					"name": "variable.language",
					"match": "\\b(appearance|geometry|initializeOnly|SFBool|MFBool|SFColor|MFColor|SFColorRGBA|MFColorRGBA|SFDouble|MFDouble|SFFloat|MFFloat|SFImage|MFImage|SFInt32|MFInt32|SFMatrix3d|MFMatrix3d|SFMatrix3f|MFMatrix3f|SFMatrix4d|MFMatrix4d|SFMatrix4f|MFMatrix4f|SFNode|MFNode|SFRotation|MFRotation|SFString|MFString|SFTime|MFTime|SFVec2d|MFVec2d|SFVec2f|MFVec2f|SFVec3d|MFVec3d|SFVec3f|MFVec3f|SFVec4f|MFVec4f)\\b"
				},
				{
					"match": "\\b([A-Za-z0-9_]+)(\\s+{|{)",
					"captures": {
						"1": { "name": "support.class" }
					}
				},
				{
					"name": "constant.numeric",
					"match": "\\b[\\d\\.\\-]+\\b"
				},
				{
					"name": "variable.parameter",
					"match": "\\b(Interchange|Full|Immersion|Interactive|[A-Za-z0-9_]+)\\b"
				},
				{
					"name": "variable.parameter",
					"match": "^(\\s*)"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.X3D",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.X3D",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.x3d"
}